{
  "name": "blinklight",
  "version": "1.4",
  "description": "LED notification of urgent tabs on Linux",
  "homepage_url": "https://github.com/snoack/chrome-blinklight",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "nativeMessaging",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "led.js",
      "blinklight.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "64": "icon.png"
  },
  "minimum_chrome_version": "42",
