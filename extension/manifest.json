{
  "name": "blinklight",
  "version": "1.4",
  "description": "LED notification of urgent tabs on Linux",
  "homepage_url": "https://github.com/snoack/chrome-blinklight",
  "permissions": [
    "tabs",
    "storage",
    "nativeMessaging",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "visible_windows.js",
      "led.js",
      "tabs.js",
      "blinklight.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["https://www.irccloud.com/"],
      "js": ["content_common.js", "content_irccloud.js"]
    },
    {
      "matches": ["https://mail.google.com/mail/*"],
      "js": ["content_common.js", "content_gmail.js"]
    },
    {
      "matches": ["https://plus.google.com/*"],
      "js": ["content_common.js", "content_google_plus.js"]
    },
    {
      "matches": ["https://talkgadget.google.com/u/*/talkgadget/_/frame*"],
      "js": ["content_common.js", "content_hangout.js"],
      "all_frames": true
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "64": "icon.png"
  },
  "minimum_chrome_version": "42",
  "manifest_version": 2,
